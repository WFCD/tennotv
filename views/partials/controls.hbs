<div class="container" id="above-player-controls">
  <div class="row">
    <div class="navbar n-center text-center">
      <button name="History" aria-label="History" type="button" id="historyTrigger" class="btn btn-secondary btn-sm" href="#history" data-target="#history" data-toggle="modal">
        <a data-toggle="tooltip" data-placement="bottom" title="Open History">
          <i class="fas fa-history"></i>
          </a>
      </button>
      <button name="Playlist" aria-label="Playlist" type="button" id="playlistTrigger" class="btn btn-secondary btn-sm" href="#plist" data-target="#plist" data-toggle="modal">
        <a data-toggle="tooltip" data-placement="bottom" title="Open Playlist">
          <i class="fas fa-list-ul"></i>
        </a>
      </button>
      <button name="Content Creators" aria-label="Content Creators" type="button" id="creatorsTrigger" class="btn btn-secondary btn-sm" href="#creators" data-target="#creators" data-toggle="modal">
        <a data-toggle="tooltip" data-placement="bottom" title="Content Creators">
          <i class="fas fa-users"></i>
        </a>
      </button>
      <button name="Next Video" aria-label="Next Video" type="button" id="playerNext" onclick="player.seekTo(player.getDuration())" class="btn btn-secondary btn-sm">
        <a data-toggle="tooltip" data-placement="bottom" title="Next Video">
          <i class="fas fa-fast-forward"></i>
        </a>
      </button>
      <button name="Like Video" aria-label="Like Video" type="button" id="playerLike" onclick="likeVideo()" class="btn btn-secondary btn-sm">
        <a data-toggle="tooltip" data-placement="bottom" title="Like Video">
          <i class="fas fa-thumbs-up"></i>
        </a>
      </button>
      <button name="Dislike Video" aria-label="Dislike Video" type="button" id="playerDislike" onclick="dislikeVideo()" class="btn btn-secondary btn-sm">
        <a data-toggle="tooltip" data-placement="bottom" title="Dislike Video">
          <i class="fas fa-thumbs-down"></i>
        </a>
      </button>
    </div>
  </div>
</div>
