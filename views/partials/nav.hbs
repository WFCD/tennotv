<!-- History Modal dialog-->
<div class="modal plist-modal" id="history" tabindex="-1" role="dialog" aria-labelledby="historyLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Watched History</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" name="Dismiss Modal">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <table class="table table-hover table-dark plist">
          <thead>
            <tr>
              <th scope="col" class="title col-md-7">Title</th>
              <th scope="col" class="author col-md-2">Owner</th>
              <th scope="col" class="tags col-md-3">Tags</th>
            </tr>
          </thead>
          <tbody id="historyList">
          </tbody>
        </table>
      </div>
      <div class="modal-footer">
        <button name="Dismiss Modal" type="button" class="btn btn-secondary btn-danger" data-dismiss="modal" style="cursor:pointer;">Close</button>
      </div>
    </div>
  </div>
</div>

<!-- playlist modal dialog -->
<div class="modal plist-modal" id="plist" tabindex="-1" role="dialog" aria-labelledby="playlistLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Playlist</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" name="Dismiss Modal">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="col-md-12" id="t-wrap">
          <table class="table table-hover table-dark plist">
            <thead>
              <tr>
                <th scope="col" class="numRow col-md-1">#</th>
                <th scope="col" class="title col-md-6">Title</th>
                <th scope="col" class="author col-md-2">Owner</th>
                <th scope="col" class="tags col-md-3">Tags</th>
              </tr>
            </thead>
            <tbody id="playlist">
            </tbody>
          </table>
        </div>
      </div>
      <div class="modal-footer">
        <button name="Dismiss Modal" type="button" class="btn btn-secondary btn-danger" data-dismiss="modal" style="cursor:pointer;">Close</button>
      </div>
    </div>
  </div>
</div>

<!-- creators modal dialog -->
<div class="modal plist-modal" id="creators" tabindex="-1" role="dialog" aria-labelledby="creatorsLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Content Creators</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" name="Dismiss Modal">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container">
          <div class="row">
            <h6 class="caption">
              Select a content creator below to view their videos.
            </h6>
          </div>
          <div class="row text-center">
            {{#each creators}}
              <div class="col-sm-2" id='creator-{{this.id}}'>
                <figure>
                  <a href="/{{this.name}}" rel="noopener" title="{{this.nameDisp}}" alt="{{this.nameDisp}}'s Page"><img src="{{this.thumb}}" height="75px" width="75px" />
                    <figcaption class="creator-name">{{this.nameDisp}}</figcaption>
                  </a>
                </figure>
              </div>
            {{/each}}
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button name="Dismiss Modal" type="button" class="btn btn-secondary btn-danger" data-dismiss="modal" style="cursor:pointer;">Close</button>
      </div>
    </div>
  </div>
</div>

