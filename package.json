{
  "name": "tennotv",
  "version": "0.0.0-development",
  "description": "Warframe YouTube Content",
  "scripts": {
    "test": "npm run pre && mocha mochaspec.js",
    "postinstall": "npm install -g gulp-cli",
    "start": "npm run pre && node ./bin/www",
    "pre": "gulp && gulp hash",
    "dev": "nodemon",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "api": "node ./server/server.js",
    "dev:server": "nodemon ./server/server.js --config server.nodemon.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WFCD/tennotv.git"
  },
  "keywords": [
    "warframe",
    "tennotv"
  ],
  "author": "TobiTenno",
  "license": "APACHE 2.0",
  "bugs": {
    "url": "https://github.com/WFCD/tennotv/issues"
  },
  "homepage": "https://tenno.tv",
  "dependencies": {
    "biguint-format": "^1.0.0",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.3",
    "debug": "~3.1.0",
    "del": "^3.0.0",
    "eslint": "^5.0.0",
    "express": "~4.16.3",
    "express-handlebars": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-cli": "^2.0.1",
    "gulp-hashsum": "^1.2.0",
    "gulp-minify": "git://github.com/zbennett10/gulp-minify.git#e1d8905041af230fd0fb980aa8a26c12c53e03f7",
    "hbs": "^4.0.1",
    "helmet": "^3.13.0",
    "nodemon": "^1.18.3",
    "serve-favicon": "~2.5.0",
    "sinon": "6.0.1",
    "snekfetch": "^4.0.4",
    "supertest": "^3.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "greenkeeper-lockfile": "^1.14.0",
    "mocha": "^5.1.1",
    "vinyl-source-stream": "^2.0.0",
    "travis-deploy-once": "^5.0.0",
    "semantic-release": "^15.5.0"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=6.1.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/js/*",
      "public/css/*",
      "public/sums.json",
      ".git",
      "server/*",
      "server.nodemon.json"
    ],
    "env": {
      "SERVICE_API_URL": "https://api.tenno.tv",
      "LOG_LEVEL": "silly"
    }
  }
}
