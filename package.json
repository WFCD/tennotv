{
  "name": "tennotv",
  "version": "0.0.0-development",
  "description": "Warframe YouTube Content",
  "scripts": {
    "test": "npm run build && mocha mochaspec.js",
    "postinstall": "npm install -g gulp-cli",
    "start": "npm run build && node app.js",
    "build": "gulp && gulp hash",
    "lint": "npx eslint assets/js/*.js routes/**/*.js app.js",
    "lint:fix": "npx eslint assets/js/*.js routes/**/*.js app.js --fix",
    "dev": "npx nodemon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WFCD/tennotv.git"
  },
  "keywords": [
    "warframe",
    "tennotv"
  ],
  "author": "TobiTenno",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/WFCD/tennotv/issues"
  },
  "homepage": "https://tenno.tv",
  "dependencies": {
    "@sentry/node": "^5.6.2",
    "biguint-format": "^1.0.0",
    "body-parser": "^1.18.2",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.3",
    "debug": "~4.1.0",
    "del": "^5.0.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-hashsum": "^1.2.0",
    "gulp-minify": "git://github.com/zbennett10/gulp-minify.git#e1d8905041af230fd0fb980aa8a26c12c53e03f7",
    "hbs": "^4.0.5",
    "helmet": "^3.21.1",
    "node-fetch": "^2.3.0",
    "serve-favicon": "~2.5.0",
    "sinon": "7.4.0",
    "supertest": "^4.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "greenkeeper-lockfile": "^1.14.0",
    "mocha": "^6.2.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=6.1.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/js/*",
      "public/css/*",
      "public/sums.json",
      ".git"
    ],
    "env": {
      "x_SERVICE_API_URL": "https://api.tenno.tv/",
      "SERVICE_API_URL": "http://localhost:3001/",
      "LOG_LEVEL": "silly",
      "RAVEN_DSN": "",
      "RAVEN_DSN_PRIVATE": "",
      "PORT": 3002
    }
  }
}
